<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NKS_Subthemeformula__c</fullName>
    <description
    >Formula field displaying the correct subtheme value. Displays either the NKS_Subtheme or the CRM_SubTheme depending on which value is set.</description>
    <externalId>false</externalId>
    <formula
    >IF(NOT(ISBLANK(NKS_Subtheme__c)), NKS_Subtheme__r.Name, TEXT(CRM_SubTheme__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Subtheme</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
