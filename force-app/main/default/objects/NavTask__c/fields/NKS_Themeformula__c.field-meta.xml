<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NKS_Themeformula__c</fullName>
    <description
    >Formula field displaying the correct theme value. Displays either the NKS_Theme or the CRM_Theme depending on which value is set.</description>
    <externalId>false</externalId>
    <formula
    >IF(NOT(ISBLANK(NKS_Theme__c)), NKS_Theme__r.Name, TEXT(CRM_Theme__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Theme</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
