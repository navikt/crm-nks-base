<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NKS_Queue_Time__c</fullName>
    <description>Number of seconds spent in queue before abandoning the chat</description>
    <externalId>false</externalId>
    <formula>(TIMEVALUE(CancelDateTime)-TIMEVALUE(RequestDateTime))/1000</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Queue Time</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
